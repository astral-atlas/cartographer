[ignore]
<PROJECT_ROOT>/build/.*
.*/\.nyc_output/.*
.*/coverage/.*
.*/node_modules/.*\.test.js
[include]

[libs]
flow-typed
[lints]
all=error
dynamic-export=off
sketchy-null-string=off
[options]
emoji=true
[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null
